
<div class="dashboard-content">
<table class="table table-bordered">
  <thead>
    <tr>
      <th>试卷名称</td>
      <th>类别</td>
      <th>题数</td>
    </tr>
  </thead>

  <tbody>
    <% @quizzes.each do |quiz| %>
    <tr>
      <td><%= link_to quiz.title, quiz_question_path(quiz, quiz.questions.first) %></td>
      <td><%= quiz.category.title %></td>
      <td><%= quiz.questions.size %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>
